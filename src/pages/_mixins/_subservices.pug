mixin subservices
	if subservices
		div.section.section-catalog
			.container
				each type, val in subservices
					.catalog-elem
						img.lazyload(src="data:image/gif;base64," data-src=''+type['img'] alt='')
						.info-block
							if type['price']
								.price= type['price']
							else
							.title= type['name']
							.buttons
								a.btn-sm__green.hvr-bob(href=''+type['pageUrl'])
									| Подробнее
								a.btn-sm__t-green.hvr-bob.modal-btn(href='#pop-up')
									| Заказать
	else
